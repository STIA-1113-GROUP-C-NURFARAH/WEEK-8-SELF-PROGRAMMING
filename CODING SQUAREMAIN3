package method;

public class SquareMain3 {

	public static void main(String[] args) {
		//A method accepting arguments and returning value
		
		SquareMain3 sm3 = new SquareMain3();
		
		int result = 0;
		
		for (int i = 1; i <= 5; i++) {
			result = sm3.getSquare(i);
			System.out.println("Square of " + i + " is : " + result );
		}
	}
	
	int getSquare(int x) {
		return x * x;

	}

}
