package method;

public class MethodCall {

	public static void main(String[] args) {
		//A method accepting no arguments and returning no value
		
		MethodCall obj = new MethodCall();
		
		System.out.println("I Love Programming");
		obj.methodName1();
		obj.methodName2();
	}
	void methodName1() {
		System.out.println("I Like Coding");
	}
	void methodName2() {
		System.out.println("I Learn Java");
		
	}

}
